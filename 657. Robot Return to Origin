bool judgeCircle(char* moves) {
    int i,s=0,a=0;
    for (i=0;moves[i]!='\0';i++)
    {
        if (moves[i]=='L')
            s=s-1;
        if (moves[i]=='R')
            s=s+1;
        if (moves[i]=='U')
            a=a+1;
        if (moves[i]=='D')
            a=a-1;
    }
    if (s==0&&a==0)
        return true;
    else
        return false;
}
