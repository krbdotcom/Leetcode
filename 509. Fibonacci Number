int fib(int n){
    int f[n+1],fib,i;
    f[0]=0;
    if(n==0)
        return f[0];
    else if (n==1)
        fib=1;
    else if (n==2)
        fib=1;
    else
    {
        f[1]=1;
        for (i=2;i<=n;i++)
        {
            f[i]=f[i-1]+f[i-2];
        }
        fib=f[n];
    }
    return fib;
}
