int missingNumber(int* nums, int numsSize) {
    int i,j,s=0,max=0;
    for (i=0;i<numsSize;i++)
    {
        for (j=0;j<numsSize;j++)
        {
            if (i==nums[j])
                s=s+1;
        }
        if (s==0)
            return i;
        s=0;
        if (max<=i)
            max=i;
    }
    if (max<numsSize)
        return max+1;
    return 0;
}
